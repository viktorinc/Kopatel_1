<Window x:Class="WpfApp1.OrderUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="OrderUi" Background="Black" Height="450" Width="434.375">
    <Grid>
        <Label Content="Your Orders" Foreground="White" HorizontalAlignment="Center" FontSize="25" FontWeight="Bold" Margin="138,10,138,364"/>
        <ListView Background="Black" ItemsSource="{Binding orders}" Foreground="White" FontSize="15" Name="Orders" Margin="10,76,10,10"/>
    </Grid>
</Window>
